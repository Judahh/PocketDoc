{
  "name": "pocketDoc",
  "productName": "pocketDoc",
  "author": "Judah <judahholanda7@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "main": "api/server.js",
  "engines": {
    "node": "9.4.6",
    "npm": "5.5.1"
  },
  "description": "pocketDoc project",
  "keywords": [
    "judahh",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Judahh/pocketDoc"
  },
  "homepage": "https://judahh.com",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "postinstall": "tsc",
    "prestart": "webpack",
    "start": "node .",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "shx rm -Rf build/",
    "gulp": "gulp",
    "startXElectron": "startx /opt/pocketDoc/node_modules/electron/dist/electron .",
    "startElectron": "electron .",
    "devElectron": "electron . --debug",
    "testElectron": "mocha && standard",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "webpack": "webpack"
  },
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/webpack": "^3.8.8",
    "async": "2.6.0",
    "backapijh": "0.2.49",
    "backappjh": "0.2.23",
    "chai": "^4.1.2",
    "css-loader": "0.28.9",
    "compression-webpack-plugin":"1.1.8",
    "d3": "4.13.0",
    "dotenv": "^5.0.0",
    "file-loader": "1.1.6",
    "flexiblepersistence": "1.0.13",
    "freeice": "^2.2.0",
    "mocha": "^5.0.1",
    "moment": "2.20.1",
    "postcss-loader": "2.1.0",
    "semaphore": "^1.1.0",
    "style-loader": "^0.20.2",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
  }
}
